package ProduttoriConsumatori;

public class Produttore extends Thread {
	private Buffer buf;
	public void Produttore( Buffer b ) { buf = b; }
	public void run(){
		for(int i = 0 ; i < 10 ; i ++) {
			try {
				buf.insert(i);
				System.out.print("Prod " + i);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
		}
	}	
}
